os: windows
app: microsoft_outlook
-
settings():
    user.context_sensitive_dictation = true
    user.paste_to_insert_threshold = 0
    
archive: key(alt h o 1)
email new: key(ctrl-n)
calendar: key(ctrl-2)
inbox: key(ctrl-1)
Reply: key(ctrl-r)
Reply all: key(ctrl-shift-r)
Forward: key(ctrl-f)
accept: key(shift-f10 c c enter)